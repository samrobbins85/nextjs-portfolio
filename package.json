    "react-dom": "^18.2.0",
    "react-simple-maps": "^3.0.0",
    "react-tooltip": "5.11.1",
    "rehype-katex": "^6.0.2",
    "rehype-prism-plus": "^1.5.1",
    "remark-behead": "^3.1.0",
    "remark-gfm": "^3.0.1",
