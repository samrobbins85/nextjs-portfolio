    "next-sitemap": "^4.0.7",
    "postcss-import": "^15.1.0",
    "tailwind-nord": "^1.3.0",
    "tailwind-outline-plugin": "^2.0.1",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4"
