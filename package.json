    "@primer/octicons-react": "^19.7.0",
    "@samrobbins/custom-typography": "^1.2.0",
    "@vercel/og": "^0.1.0",
    "d3-scale": "4.0.2",
    "dotenv": "^16.0.3",
    "eslint-config-prettier": "^8.8.0",
    "feed": "^4.2.2",
    "graphql": "^16.6.0",
    "graphql-request": "5.1.0",
    "katex": "^0.16.6",
    "next": "^13.3.0",
    "next-mdx-remote": "^4.4.1",
    "next-themes": "^0.2.1",
    "node-fetch": "^3.3.1",
    "postcss": "^8.4.23",
